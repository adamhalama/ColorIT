<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="view.ProjectViewController">
   <Button mnemonicParsing="false" onAction="#openRequirement" text="Project1">
      <VBox.margin>
         <Insets bottom="30.0" left="30.0" right="30.0" top="30.0"/>
      </VBox.margin>
   </Button>
   <Button mnemonicParsing="false" onAction="#openRequirement" text="Project2">
      <VBox.margin>
         <Insets bottom="30.0" left="30.0" right="30.0"/>
      </VBox.margin>
   </Button>
   <Button layoutX="40.0" layoutY="40.0" mnemonicParsing="false"
           onAction="#openTeam" text="openTeam"/>
</VBox>
