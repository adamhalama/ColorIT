<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="358.0" prefWidth="599.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.TeamMemberViewController">
   <TableView fx:id="teamMemberList" prefHeight="297.0" prefWidth="594.0">
     <columns>
       <TableColumn fx:id="nameColumn" prefWidth="280.0" text="name" />
       <TableColumn fx:id="emailColumn" minWidth="0.0" prefWidth="311.0" text="e-mail" />
     </columns>
   </TableView>
   <HBox prefHeight="43.0" prefWidth="599.0">
      <children>
         <Button mnemonicParsing="false" onAction="#addTeamMember" text="add">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </Button>
         <Button mnemonicParsing="false" text="get more information">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </Button>
         <Button mnemonicParsing="false" onAction="#editTeamMember" text="edit">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </Button>
         <Button mnemonicParsing="false" onAction="#deleteTeamMember" text="delete">
            <HBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
         </Button>
      </children>
   </HBox>
   <Label text="error label" textFill="RED" fx:id="errorLabel"/>

</VBox>
