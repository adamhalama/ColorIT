<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="240.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ManageProjectViewController">
   <children>
      <Label text="Name:" />
      <TextField fx:id="name" />
      <Label text="Description:" />
      <TextArea fx:id="description" prefHeight="102.0" prefWidth="600.0" />
      <HBox prefHeight="44.0" prefWidth="600.0">
         <children>
            <Button mnemonicParsing="false" onAction="#save" text="save" />
            <Button mnemonicParsing="false" onAction="#cancel" text="cancel" />
         </children>
      </HBox>
      <Label text="error label" textFill="RED" fx:id="errorLabel"/>
   </children>
</VBox>
