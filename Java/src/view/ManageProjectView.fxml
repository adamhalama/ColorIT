<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="240.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ManageProjectViewController">
   <children>
      <Label text="Name:" />
      <TextField fx:id="name" />
      <Label text="Description:" />
      <TextArea fx:id="description" prefHeight="102.0" prefWidth="600.0" />
      <HBox prefHeight="44.0" prefWidth="600.0">
         <children>
            <Button mnemonicParsing="false" onAction="#save" text="Save">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#cancel" text="Cancel">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
      <Label fx:id="errorLabel" text="error label" textFill="RED">
         <VBox.margin>
            <Insets left="5.0" />
         </VBox.margin></Label>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
