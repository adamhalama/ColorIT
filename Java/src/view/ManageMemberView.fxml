<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ManageMembersViewController">
    <children>
        <Label text="Name:" />
        <TextField fx:id="name" />
        <Label text="Email:" />
      <TextField fx:id="email" />
        <HBox prefHeight="44.0" prefWidth="600.0">
            <children>
                <Button mnemonicParsing="false" onAction="#save" text="save">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin></Button>
                <Button mnemonicParsing="false" onAction="#cancel" text="cancel">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin></Button>
            </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
        </HBox>
        <Label fx:id="errorLabel" text="error label" textFill="RED" />
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>

</VBox>
